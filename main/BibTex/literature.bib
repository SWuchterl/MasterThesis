 series = {2053-2571},
 type = {Book Chapter},
 pages = {3-1 to 3-9},
 abstract = {The Large Hadron Collider spans 27 km in circumference, crossing the French–Swiss border four times. It is hosted in a nearly circular tunnel buried at an average depth of 100 m below ground. It accomplishes the very non-trivial task of producing head-on collisions between protons while they travel in opposite directions at nearly the speed of light. Furthermore, the collisions are set to happen within centimeters of designated locations along the tunnel. This no small feat either, with the protons traveling at almost at the speed of light, having them collide a few nanoseconds too early or too late would miss the target by meters.},
 url = {http://dx.doi.org/10.1088/978-1-6817-4078-2ch3},
